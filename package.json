{
  "name": "rss-dumper",
  "version": "1.0.0",
  "description": "rss dumper",
  "author": "Soraki Hirano",
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --watch 'build/**/*.ts' --exec ts-node src/app.ts",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:studio": "prisma studio",
    "test": "NODE_ENV=test jest --runInBand --collect-coverage",
    "tsoa:swagger": "tsoa spec",
    "tsoa:routes": "tsoa routes",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "@tsoa/runtime": "^5.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "sanitize-html": "^2.11.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.3",
    "@types/sanitize-html": "^2.9.4",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prisma": "5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
