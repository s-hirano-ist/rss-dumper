{
  "name": "rss-dumper",
  "type": "module",
  "version": "0.0.1",
  "description": "RSS Dumper Project.",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec yarn node --loader ts-node/esm src/app.ts",
    "start": "yarn node --loader ts-node/esm src/app.ts",
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:studio": "prisma studio",
    "test": "NODE_ENV=test jest --runInBand --collect-coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^6.0.1",
    "@adminjs/prisma": "^5.0.1",
    "@prisma/client": "5.3.1",
    "adminjs": "^7.2.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "joi": "^17.10.2",
    "sanitize-html": "^2.11.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/sanitize-html": "^2.9.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prisma": "5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "yarn node --loader ts-node/esm prisma/seed.ts"
  }
}
